      <img src="http://www.hanzevastcapital.nl/skins/default/images/hanzevast.png" alt="Hanzevast"/>
    {{/header.avatar}}
    {{#header.text as |text|}}
      {{#text.title}}Quintushuis{{/text.title}}
      {{#text.subhead}}Schweitzerlaan 12 Groningen{{/text.subhead}}
    {{/header.text}}
  {{/card.header}}
 --}}
  {{#card.title as |title|}}
